<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
    <suppress files="[\\/].*(package-info)\.java" checks="RegexpOnFilename"/>

    <suppress files=".*[\\/]core[\\/]constant([\\/])?.*" checks="InterfaceIsType"/>

    <suppress files=".*[\\/]gmssl[\\/]jni([\\/])?.*" checks="ParameterName"/>
    <suppress files=".*[\\/]gmssl[\\/]jni([\\/])?.*" checks="ParameterNumber"/>
    <suppress files=".*[\\/]gmssl[\\/]jni([\\/])?.*" checks="InterfaceIsType"/>
</suppressions>
